combonum.sh
for i in 1 2 3
do
for j in 1 2 3
do
for k in 1 2 3
do
echo "$i $j $k"
done 
done
done


fibseries.sh
echo "enter the number n:"
read n
x=0
y=1
i=2
echo "fib series upto $n terms"
echo "$x"
echo "$y"
while [ $i -lt $n ];do
i=`expr $i+1`
z=`expr $x + $y`
echo "$z"
x = $y
y = $z
done


powerofnum.sh
#power of a number
echo "enter base number and pos integer as the power"
read m n 
c = $ (( $m ** $n ))
echo "$m raised to the power of $n is: $c"

sumofnnumbers.sh
echo "enter the number of n "
read n
sum = 0
for ((i=0; i<=n; i++))
do
  sum = $((sum+i))
done
echo "sum of first N number is $num"

